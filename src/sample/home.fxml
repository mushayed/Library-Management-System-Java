<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.HomeController">
   <children>
      <Button fx:id="buttonMemberList" layoutX="250.0" layoutY="147.0" mnemonicParsing="false" onAction="#memberAction" text="Member " textFill="#d70909">
         <font>
            <Font size="18.0" />
         </font></Button>
      <Button fx:id="buttonBookList" layoutX="265.0" layoutY="200.0" mnemonicParsing="false" onAction="#bookAction" text="Book " textFill="#ee0d0d">
         <font>
            <Font size="18.0" />
         </font></Button>
      <Button fx:id="activityButton" layoutX="223.0" layoutY="249.0" mnemonicParsing="false" onAction="#activityAction" text="Member Activity" textFill="#e80404">
         <font>
            <Font size="18.0" />
         </font></Button>
      <Button fx:id="buttonLogout" layoutX="260.0" layoutY="301.0" mnemonicParsing="false" onAction="#logoutAction" text="Logout" textFill="#ee0707">
         <font>
            <Font size="18.0" />
         </font></Button>
      <Label fx:id="authorName" layoutX="226.0" layoutY="66.0" text="Label">
         <font>
            <Font size="38.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
