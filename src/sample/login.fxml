<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LoginController">
   <children>
      <Label layoutX="159.0" layoutY="102.0" text="Name" textFill="#d71010">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label layoutX="159.0" layoutY="163.0" text="Password" textFill="#e40a0a">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <PasswordField fx:id="passwordField" layoutX="288.0" layoutY="166.0" />
      <TextField fx:id="nameField" layoutX="288.0" layoutY="104.0" />
      <Button fx:id="loginButton" layoutX="380.0" layoutY="228.0" mnemonicParsing="false" onAction="#loginButtonAction" text="Login" textFill="#830202">
         <font>
            <Font size="15.0" />
         </font></Button>
      <Button fx:id="backButton" layoutX="312.0" layoutY="228.0" mnemonicParsing="false" onAction="#backAction" text="Back" textFill="#790303">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Label fx:id="loginLabel" layoutX="239.0" layoutY="36.0" text="Login Page" textFill="#8a0303">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
