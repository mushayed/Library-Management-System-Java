<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ActivityController">
   <children>
      <Label layoutX="204.0" layoutY="35.0" text="Member Activity View" textFill="#720303">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TableView fx:id="activityTable" layoutX="10.0" layoutY="88.0" prefHeight="200.0" prefWidth="577.0">
      </TableView>
      <Button fx:id="viewButton" layoutX="275.0" layoutY="306.0" mnemonicParsing="false" onAction="#viewAction" text="View" textFill="#6f0202">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="backButton" layoutX="275.0" layoutY="355.0" mnemonicParsing="false" onAction="#backAction" text="Back" textFill="#6f0404">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
